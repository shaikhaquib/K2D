<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.abatechnology.kirana2door">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="com.abatechnology.kirana2door.api.AppController"
        android:allowBackup="false"
        android:icon="@drawable/kiranaicon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:allowBackup">
        <activity android:name="com.abatechnology.kirana2door.activities.SearchActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name="com.abatechnology.kirana2door.activities.PrivacyPolicy"
            android:screenOrientation="portrait"/>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name="com.abatechnology.kirana2door.activities.StockistDetail"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.SubOrder"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.AboutUs"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.ContactVerification"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.UpdateContact"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.ManageAddress"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.PersonalInfo"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.ProductDetail"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.abatechnology.kirana2door.activities.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.abatechnology.kirana2door.activities.CartProductList"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.Product_page"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.ChangePassword"
            android:screenOrientation="portrait"/>
        <activity
            android:name="com.abatechnology.kirana2door.activities.LoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.abatechnology.kirana2door.activities.Home"
            android:screenOrientation="portrait" />
        <activity android:name="com.abatechnology.kirana2door.activities.CustomerRegistration"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.OTPVerification"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.ForgotPassword"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.ForgotPassVerification"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.SetNewPassword"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.BillingAddActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.PaymentActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.abatechnology.kirana2door.activities.OrderSuccess"
            android:screenOrientation="portrait"/>
    </application>

</manifest>